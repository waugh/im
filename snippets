# Introduction of modules to the project.

http://www.asyncdev.net/2012/08/locking-node-js-module-versions-with-shrinkwrap/

nvm use
nvm install
npm install --save coffee-script
npm shrinkwrap

# Occasional study.

nvm use
node_modules/.bin/coffee --compile compiler.coffee
node_modules/.bin/coffee --compile interpreter.coffee
rm compiler.js
rm interpreter.js

# Routine testing.

nvm use
node_modules/.bin/coffee

keys_of = Object.keys
keys_of support = require './support'
load = support.load

keys_of support = load './support'
load = support.load

load './test'


keys_of scheduler = load './scheduler'
trampoline = imperatrix_mundi.trampoline # a global set by scheduler.
Event = scheduler.Event
ex = Event.new()
ex.register -> console.log "izzatso"
ex.register -> console.log "Are you sure?"
ex.fire()
trampoline.exhaust()
ex.register -> console.log "Already fired."

keys_of global_variable = load './global_variable'
imperatrix_mundi is global_variable.the

keys_of compiler = load './compiler'
compiler.infect imperatrix_mundi # trashes symbol table.
symbol = imperatrix_mundi.symbol

keys_of interpreter = load './interpreter'
LogicalVariable = interpreter.LogicalVariable
ex = LogicalVariable.new()

keys_of imperatrix_mundi




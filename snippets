# Introduction of modules to the project.

http://www.asyncdev.net/2012/08/locking-node-js-module-versions-with-shrinkwrap/

nvm use
nvm install
npm install --save coffee-script
npm shrinkwrap

# Occasional study.

nvm use
node_modules/.bin/coffee --compile compiler.coffee
node_modules/.bin/coffee --compile interpreter.coffee
rm compiler.js
rm interpreter.js

# Routine testing.

nvm use
node_modules/.bin/coffee

Object.keys support = require './support'
load = support.load

Object.keys support = load './support'
load = support.load

Object.keys global_variable = load './global_variable'
imperatrix_mundi is global_variable.the

Object.keys compiler = load './compiler'
compiler.infect imperatrix_mundi # trashes symbol table.
symbol = imperatrix_mundi.symbol

Object.keys interpreter = load './interpreter'
literal = interpreter.literal
cons = interpreter.make_cons()
interpreter.exercise_cons()

Object.keys imperatrix_mundi

